{
  "breadcrumbs.enabled": false,
  "editor.renderWhitespace": "none",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.fontSize": 15.4,
  "diffEditor.ignoreTrimWhitespace": true,
  "liveServer.settings.donotVerifyTags": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "ejs": "html"
  },
  "terminal.integrated.env.windows": {},
  "emmet.excludeLanguages": [
    "markdown"
  ],
  "[json]": {
    "editor.quickSuggestions": {
      "strings": false,
      "comments": false,
      "other": false
    },
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.linkedEditing": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.decorations.badges": true,
  "workbench.iconTheme": "symbols",
  "explorer.decorations.colors": true,
  "explorer.decorations.badges": true,
  "git.ignoreMissingGitWarning": true,
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.startupEditor": "none",
  "editor.stickyTabStops": true,
  "editor.tabSize": 2,
  "git.enableSmartCommit": true,
  "[dart]": {
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
  },
  "editor.unicodeHighlight.allowedCharacters": {
    "ุง": true,
    "ู": true
  },
  "files.autoSave": "afterDelay",
  "explorer.compactFolders": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.scrollbar.verticalScrollbarSize": 19,
  "editor.scrollbar.horizontalScrollbarSize": 19,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.detectIndentation": false,
  "explorer.copyRelativePathSeparator": "/",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.openRepositoryInParentFolders": "never",
  "editor.unicodeHighlight.allowedLocales": {
    "es": true
  },
  "editor.guides.bracketPairs": "active",
  "window.zoomLevel": 0.95,
  "editor.fontFamily": "Cascadia Code, Consolas, 'Courier New', monospace",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "block",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.lineHeight": 0.8,
  "prettier.requireConfig": false,
  "prettier.printWidth": 80,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "avoid",
  "prettier.proseWrap": "preserve",
  "prettier.endOfLine": "lf",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.suggestSelection": "first",
  "editor.hover.sticky": true,
  "terminal.integrated.cursorBlinking": true,
  "editor.cursorBlinking": "smooth",
  "terminal.integrated.smoothScrolling": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.editor.enablePreview": true,
  "explorer.confirmPasteNative": false,
  "terminal.integrated.tabs.enableAnimation": true,
  "explorer.fileNesting.enabled": true,
  "workbench.list.smoothScrolling": true,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 40,
  "workbench.sideBar.location": "left",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "editor.lightbulb.enabled": "onCode",
  "editor.stickyScroll.enabled": true,
  "workbench.tree.expandMode": "singleClick",
  "workbench.editor.closeOnFileDelete": true,
  "editor.selectionHighlight": true,
  "editor.folding": true,
  "editor.parameterHints.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.mouseWheelScrollSensitivity": 0.9,
  "symbols.hidesExplorerArrows": false,
  "workbench.preferredDarkColorTheme": "1984 - Cyberpunk",
  "workbench.preferredHighContrastColorTheme": "1984 - Cyberpunk",
  "workbench.preferredHighContrastLightColorTheme": "1984 - Cyberpunk",
  "workbench.preferredLightColorTheme": "1984 - Cyberpunk",
  /*Supermaven*/
  "supermaven.enable": {
    "supermaven.autoComplete.enabled": false,
    "supermaven.codeAnalysis.maxFileSizeMB": 0.25,
    "supermaven.suggestions.frequency": "low",
    "*": true,
    "supermaven.completions.maxItems": 10,
    "supermaven.suggestions.maxResults": 5,
    "supermaven.validateOnSave": false,
    "supermaven.validateOnType": false,
    "supermaven.analysis.dependencyScan": false,
    "supermaven.visualization.diagram": false,
    "supermaven.concurrency.maxProcesses": 2,
    "supermaven.concurrency.maxThreads": 2,
    "supermaven.concurrency.maxConnections": 2,
    "supermaven.concurrency.maxSockets": 2,
    "supermaven.analysis.maxFileSizeMB": 0.25,
    "supermaven.analysis.maxFileSize": 25,
    "supermaven.analysis.maxRecursion": 10,
    "supermaven.allowGitignore": true,
  },
  "terminal.integrated.scrollback": 1000,
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.fastScrollSensitivity": 10,
  "terminal.integrated.cursorStyle": "block",
  "editor.codeLens": false,
  "editor.colorDecorators": true,
  "security.workspace.trust.untrustedFiles": "open",
  "tailwindCSS.experimental.memoryUsage": "low",
  "editor.renderControlCharacters": true,
  "editor.scrollBeyondLastColumn": 5,
  "editor.scrollBeyondLastLine": true,
  //otros
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "update.enableWindowsBackgroundUpdate": false,
  "search.followSymlinks": false,
  "explorer.autoReveal": false,
  "workbench.colorTheme": "1984 - Cyberpunk",
  "editor.lineHeight": 20
}